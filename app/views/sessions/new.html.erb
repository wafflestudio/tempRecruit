
<h1>Login </h1>


<%= form_for :session, url: sessions_path, method: :post do |f| %>

  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div>
    <%= f.submit "Create an account" %>
  </div>

<% end %>




